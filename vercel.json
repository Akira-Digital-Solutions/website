{
    "version": 2,
    "builds": [
        { "src": "public/**/*", "use": "@vercel/static" },
        { "src": "src/**/*", "use": "@vercel/static" },
        { "src": "components/**/*", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/public/$1" },
        { "src": "/(images|fonts|video|icons)/(.*)", "dest": "/src/$1/$2" }
    ]
}
